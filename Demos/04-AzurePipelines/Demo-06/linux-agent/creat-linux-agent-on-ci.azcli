rnd=$RANDOM
# acr=integrationsonline
grp=az400-m04-aciagent-$rnd
loc=westeurope
img='arambazamba/aciagentlinux'
agent=aci-linux-agent-$rnd
# update org name
org='https://dev.azure.com/az-400-v2'
# replace the token
# token=ae3ypool6mvpf7624it7j4smbveyrics4rnpulaqbjkeijaxl6nq
pool=aci-pool

az group create -n $grp -l $loc

az container create -g $grp -l $loc -n $agent --image $img --cpu 1 --memory 1 --dns-name-label $agent --port 80 --environment-variables 'AZP_URL'=$org 'AZP_TOKEN'=$token 'AZP_AGENT_NAME'=$agent 'AZP_POOL'=$pool
